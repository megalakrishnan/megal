import java.util.Scanner;

public class simple {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		if(a==0)
		{
			System.out.println("Zero");
		}
		if(a>0)
		{
			System.out.println("Positive");
		}
	    if(a<0)
	    {
	    	System.out.println("Negative");
	    }
	}

}
